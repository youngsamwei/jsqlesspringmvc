

{
	//  Extension ID:oeejofojochggegmkbmjbjhiojakbcme
	"manifest_version": 2,

	"name": "Invoke Native Application",
	"description": "example",
	"version": "1.0",

	"browser_action": {
		"default_icon" : "icon.png"
		/*,"default_popup": "popup.html"*/
	},

	"background" : { "scripts": ["background.js"] },

	"permissions" : [
		"nativeMessaging",
		"tabs",
		"http://localhost:8080/*"
	],
	"content_scripts": [
		{
		"matches": ["http://localhost:8080/*"],
		"js": ["content.js"]
		}
	]
}